{
    "name": "goksagun/scheduler-bundle",
    "description": "Scheduled task bundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Burak Bolat",
            "email": "brkblt@gmail.com"
        }
    ],
    "keywords": [
        "scheduler",
        "task",
        "bundle"
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": "^8.1",
        "ext-mbstring": "*",
        "doctrine/annotations": "^1.4",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.5",
        "symfony/framework-bundle": "~6.0",
        "symfony/console": "~6.0",
        "symfony/yaml": "~6.0",
        "symfony/process": "~6.0",
        "dragonmantank/cron-expression": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Goksagun\\SchedulerBundle\\": "src/"
        },
        "files": [
            "config/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Goksagun\\SchedulerBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    },
    "config": {
        "sort-packages": true
    }
}
