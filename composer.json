{
    "name": "goksagun/scheduler-bundle",
    "description": "Scheduled task bundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Burak Bolat",
            "email": "brkblt@gmail.com"
        }
    ],
    "keywords": [
        "scheduler task bundle"
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "doctrine/annotations": "^1.4",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.5",
        "symfony/framework-bundle": "~4.0|~5.0",
        "symfony/console": "~4.0|~5.0",
        "symfony/yaml": "~4.0|~5.0",
        "symfony/process": "~4.0|~5.0",
        "dragonmantank/cron-expression": "^2.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Goksagun\\SchedulerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Goksagun\\SchedulerBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "symfony": {
            "allow-contrib": "true"
        }
    },
    "config": {
        "sort-packages": true
    }
}
